# Example environment configuration for profilsmkn4-backend
# Copy this file to `.env` and fill in the real values.
# Do not commit your real `.env` to source control.

# Postgres connection string (Prisma uses this)
# Format: postgresql://USER:PASSWORD@HOST:PORT/DBNAME
DATABASE_URL="postgresql://postgres:password@localhost:5432/profilsmkn4"

# App port
PORT=3001

# JWT secrets
JWT_SECRET="your_jwt_secret_here"
REFRESH_TOKEN_SECRET="your_refresh_token_secret_here"

# Runtime environment: development | test | production
NODE_ENV="development"

# API key for image service (if used)
IMAGE_SERVICE_API_KEY="your_image_service_api_key_here"
